<script>

    function GetUser(requestType) {
        ////This id is in the Users view page and i use it for changing users title
        //document.getElementById('UsersTitle').textContent = Title;
        $.ajax({
            type: 'Get',
            url: "/Admin/UserManager/GetAllUser",
            data: { RequestType: requestType },
            success: function (result) {
                $('#loaduser').html(result);
                $('#datatable').DataTable();
            }
        })
    }
</script>